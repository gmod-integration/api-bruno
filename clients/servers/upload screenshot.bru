meta {
  name: upload screenshot
  type: http
  seq: 2
}

post {
  url: {{baseURL}}/{{apiVersion}}/clients/{{steamID64}}/servers/{{serverID}}/screenshots
  body: json
  auth: bearer
}

auth:bearer {
  token: {{playerToken}}
}

body:json {
  {
    "player": {{player}},
    "screenshot": "",
    "captureData": {
      "format": "jpeg",
      "x": 0,
      "y": 0,
      "w": 1080,
      "h": 1920,
      "quality": 95
    },
    "size": "380KB"
  }
}
