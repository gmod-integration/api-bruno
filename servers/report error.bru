meta {
  name: report error
  type: http
  seq: 4
}

post {
  url: {{baseURL}}/{{apiVersion}}/servers/{{serverID}}/errors
  body: json
  auth: bearer
}

auth:bearer {
  token: {{serverToken}}
}

body:json {
  {
    "error": "addons/gmod_integration/[...]/sh_errors.lua:26: '=' expected near '<eof>'",
    "realm": "realm",
    "stack": "/",
    "name": "gmod_integration",
    "id": "0123456789",
    "uptime": 50,
    "identifier": "{{serverID}}"
  }
}
